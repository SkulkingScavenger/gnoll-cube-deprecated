            <kind>1</kind>
            <string>///Process Keyboard Input
if(confirm_btn){
  ///go to next room
  switch(position){
  case 0://Single Player
    sound_play(snd_click)
    crnt_pnum = pnum[0];
    global.master = true;
    global.roomprevious = room_mainmenu;
    room_goto(room_charactermain);
    break;
  case 1://Multi Player
    sound_play(snd_click)
    room_goto(room_multiplayermenu)
    global.roomprevious = room_lobby;
    break;
  case 2://Characters
    sound_play(snd_click)
    room_goto(room_charactermain)
    global.roomprevious = room_charactermain;
    break;
  case 3://Options
    break;
  case 4://Exit
    sound_play(snd_click);
    game_end();
    break;
  }
  io_clear2();
}

if(arrow_up){
  if (position &gt; 0){
    y -= 32;
    position -= 1;
  }else{
    y+=128;
    position = 4;
  }
  io_clear2();
}

if(arrow_down){
  if (position &lt; 4){
    y+=32;
    position += 1;
  }else{
    y-=128;
    position = 0;
  }
  io_clear2();
}

</string>
